<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="campusshark.dhruv.swiftly.MainActivity">

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/btn_cabs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="95dp"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        fancy:fb_borderColor="#FFFFFF"
        fancy:fb_borderWidth="1dp"
        fancy:fb_defaultColor="#7ab800"
        fancy:fb_focusColor="#9bd823"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="30dp"
        fancy:fb_text=" C    A     B     S "
        fancy:fb_textColor="#FFFFFF" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/btn_nearby_places"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_cabs"
        android:layout_alignStart="@+id/btn_cabs"
        android:layout_below="@+id/btn_cabs"
        android:layout_marginTop="36dp"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        fancy:fb_borderColor="#FFFFFF"
        fancy:fb_borderWidth="1dp"
        fancy:fb_defaultColor="#7ab800"
        fancy:fb_focusColor="#9bd823"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="30dp"
        fancy:fb_text=" NEARBY PLACES   "
        fancy:fb_textColor="#FFFFFF" />


    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/btn_live_tracking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_cabs"
        android:layout_alignStart="@+id/btn_cabs"
        android:layout_below="@+id/btn_nearby_places"
        android:layout_marginTop="36dp"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        fancy:fb_borderColor="#FFFFFF"
        fancy:fb_borderWidth="1dp"
        fancy:fb_defaultColor="#7ab800"
        fancy:fb_focusColor="#9bd823"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="30dp"
        fancy:fb_text=" LIVE TRACKING   "
        fancy:fb_textColor="#FFFFFF" />

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/multiple_actions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="3dp"
        android:background="@drawable/gym"
        fab:fab_addButtonColorNormal="@color/teal1"
        fab:fab_addButtonColorPressed="@color/teal1"
        fab:fab_addButtonPlusIconColor="@color/uber_white"
        fab:fab_labelStyle="@style/menu_labels_style">

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/gallery"
            fab:fab_colorNormal="@color/orange"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Call a cab" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/material_blue_500"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Nearby Places" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/cpb_green"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Register for Live Tracking" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/orange"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Start Live Tracking" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/colorAccent"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Grievance" />


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_f"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/cpb_green"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Crisis Management" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_g"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/nearme"
            fab:fab_colorNormal="@color/orange"
            fab:fab_colorPressed="@color/colorPrimary"
            fab:fab_title="Digital Storage" />

    </com.getbase.floatingactionbutton.FloatingActionsMenu>

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Button" />

    <Button
        android:id="@+id/crisis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/button2"
        android:text="Button" />


</RelativeLayout>
